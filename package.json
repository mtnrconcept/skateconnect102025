{
    "name": "skateconnect",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
      "dev": "vite",
      "build": "vite build",
      "lint": "eslint .",
      "preview": "vite preview",
      "typecheck": "tsc --noEmit -p tsconfig.app.json",
      "build:test": "tsc --project tsconfig.tests.json",
      "test": "VITE_SUPABASE_URL=http://localhost VITE_SUPABASE_ANON_KEY=anon npm run build:test && VITE_SUPABASE_URL=http://localhost VITE_SUPABASE_ANON_KEY=anon node --test dist-tests/tests",
      "import:swiss-spots": "node scripts/import_swiss_spots.mjs",
      "import:csv": "node scripts/import_csv_spots.mjs",
      "build:overlay": "node scripts/build_csv_overlay.mjs",
      "replace:spots": "node scripts/replace_spots_from_csv.mjs",
      "cleanup:blob-media": "node scripts/cleanup_blob_media.mjs",
      "cap:init": "npx cap init",
      "cap:add:ios": "npx cap add ios",
      "cap:add:android": "npx cap add android",
      "cap:sync": "npm run build && npx cap sync",
      "cap:copy": "npx cap copy",
      "cap:open:ios": "npx cap open ios",
      "cap:open:android": "npx cap open android",
      "android": "npm run cap:sync && npx cap open android",
      "ios": "npm run cap:sync && npx cap open ios",
      
        
          "rt:smoke:sql": "echo 'Lance le script SQL dans le dashboard Supabase'",
          "rt:smoke:node": "node scripts/realtime_smoketest.mjs"
       
    },
